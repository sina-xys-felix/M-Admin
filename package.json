{
  "name": "m-admin",
  "description": "M stands for Modular, symbolizing the modular design of the framework.",
  "version": "1.0.0",
  "private": true,
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "prod": "vite --config ./config/vite.config.prod.ts",
    "build": " vite build --config ./config/vite.config.dev.ts",
    "build:prod": " vite build --config ./config/vite.config.prod.ts",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build:prod && vite preview --host",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint-staged": "npx lint-staged",
    "lint:fix": "eslint --ext .vue,.ts src/ --fix",
    "prettier": "prettier --write ."
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
